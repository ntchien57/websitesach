{
    "name": "lanhktc/laravel-ecommerce",
    "description": "E-commerce from The Laravel Framework.",
    "keywords": ["E-commerce", "CMS", "framework", "laravel"],
    "license": "MIT",
    "authors": [{
        "name": "KTC",
        "email": "lanhktc@gmail.com"
    }],
    "type": "project",
    "require": {
        "php": ">=7.2.5",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "encore/laravel-admin": "^1.8.10",
        "fideloper/proxy": "~4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "hardevine/shoppingcart": "^3.2",
        "laravel/framework": "8.0.0",
        "laravel/helpers": "^1.6",
        "laravel/tinker": "~2.0",
        "laravel/ui": "^2.0",
        "maatwebsite/excel": "^3.1",
        "mad-web/laravel-robots": "^3.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "phpoffice/phpspreadsheet": "^1.28",
        "spatie/laravel-sitemap": "^5.8",
        "spatie/schema-org": "^3.9",
        "unisharp/laravel-filemanager": "^1.9"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.9.1",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~8.0",
        "symfony/thanks": "^1.0",
        "nunomaduro/collision": "^4.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Myapp/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },

    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
